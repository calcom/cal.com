{
  "name": "@calcom/platform-types",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.js",
  "private": true,
  "scripts": {
    "build": "yarn clean && tsc --build --force tsconfig.json",
    "build:swc": "yarn clean && swc . -d ./dist --config-file .swcrc --ignore '**/*.d.ts,dist/**,node_modules/**,.turbo/**' && npx tsc --build --force tsconfig.types.json",
    "clean": "rm -rf ./dist",
    "build:watch": "tsc --build --force ./tsconfig.json --watch",
    "post-install": "yarn build"
  },
  "dependencies": {
    "@calcom/platform-constants": "workspace:*",
    "@calcom/platform-enums": "workspace:*",
    "@nestjs/common": "10.4.20",
    "@nestjs/swagger": "7.4.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.3",
    "libphonenumber-js": "1.10.49",
    "luxon": "3.4.4",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@swc/cli": "0.6.0",
    "@swc/core": "1.11.24",
    "@types/express": "4.17.21"
  }
}
