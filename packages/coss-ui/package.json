{
  "name": "@coss/ui",
  "private": true,
  "version": "0.0.0",
  "exports": {
    "./styles.css": "./styles.css",
    "./lib/*": "./lib/*.ts",
    "./components/*": "./components/*.tsx",
    "./hooks/*": "./hooks/*.ts"
  },
  "scripts": {
