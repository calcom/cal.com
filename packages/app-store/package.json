{
  "name": "@calcom/app-store",
  "private": true,
  "sideEffects": false,
  "version": "0.0.0",
  "main": "./index.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./types/*",
        "./*"
      ]
    }
  },
  "files": [
    "components.ts"
  ],
  "scripts": {
    "build:types": "tsc --project tsconfig.build.json",
    "type-check": "tsc --pretty --noEmit",
    "type-check:ci": "tsc-absolute --pretty --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "lint:report": "biome lint --reporter json . > ../../lint-results/app-store.json"
  },
  "dependencies": {
    "@calcom/dailyvideo": "workspace:*",
    "@calcom/dayjs": "workspace:*",
    "@calcom/features": "workspace:*",
    "@calcom/lib": "workspace:*",
    "@calcom/office365video": "workspace:*",
    "@calcom/ui": "workspace:*",
    "@calcom/zoomvideo": "workspace:*",
    "lodash": "4.17.23",
    "qs-stringify": "1.2.1",
    "react-i18next": "12.3.1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "stripe": "^9.0.0 || ^15.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@calcom/testing": "workspace:*",
    "@calcom/types": "workspace:*"
  }
}
