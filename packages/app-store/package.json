  "exports": {
    ".": "./src/index.ts",
    "./apps/*": "./apps/*",
    "./generated/*": "./generated/*",
    "./src/*": "./src/*",
    "./_components/*": "./src/_components/*",
    "./_utils/*": "./src/_utils/*",
    "./_pages/*": "./src/_pages/*",
    "./templates/*": "./src/templates/*",
    "./appStoreMetaData": "./src/appStoreMetaData.ts",
    "./bookerAppsMetaData": "./src/bookerAppsMetaData.ts",
    "./constants": "./src/constants.ts",
    "./delegationCredential": "./src/delegationCredential.ts",
    "./eventTypeAppCardZod": "./src/eventTypeAppCardZod.ts",
    "./getAppAssetFullPath": "./src/getAppAssetFullPath.ts",
    "./getNormalizedAppMetadata": "./src/getNormalizedAppMetadata.ts",
    "./getVideoAdapters": "./src/getVideoAdapters.ts",
    "./locations": "./src/locations.ts",
    "./server": "./src/server.ts",
    "./utils": "./src/utils.ts",
    "./zod-utils": "./src/zod-utils.ts",
    "./types": "./src/types.d.ts",
    "./_appRegistry": "./src/_appRegistry.ts",
    "./AppDependencyComponent": "./src/AppDependencyComponent.tsx",
    "./BookingPageTagManager": "./src/BookingPageTagManager.tsx",
    "./EventTypeAppContext": "./src/EventTypeAppContext.tsx",
    "./InstallAppButton": "./src/InstallAppButton.tsx",
    "./InstallAppButtonWithoutPlanCheck": "./src/InstallAppButtonWithoutPlanCheck.tsx"
  },
  "scripts": {
    "type-check": "tsc --pretty --noEmit",
    "type-check:ci": "tsc-absolute --pretty --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "lint:report": "biome lint --reporter json . > ../../lint-results/app-store.json"
