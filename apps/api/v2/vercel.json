{
  "version": 2,
  "buildCommand": "cd ../../.. && yarn turbo run build --filter=@calcom/api-v2",
  "outputDirectory": "dist/apps/api/v2",
  "installCommand": "yarn install",
  "functions": {
    "src/main.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/main.js"
    }
  ]
}
