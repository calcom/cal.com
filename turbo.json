    "@calcom/web#dx": {
      "cache": false,
      "dependsOn": ["@calcom/prisma#dx"]
    },
    "@calcom/web#start": {
      "cache": false,
      "dependsOn": []
    },
    "@calcom/atoms#build-npm": {
      "outputs": ["dist/**", "globals.min.css"]
    },
    "@calcom/api-v2#build": {
