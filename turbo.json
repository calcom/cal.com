{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "@calcom/web#dev": {
      "cache": false,
      "dependsOn": [],
      "outputs": [".next/**"]
    },
    "@calcom/prisma#generate-schemas": {
      "cache": false,
      "dependsOn": [],
      "outputs": ["node_modules/.prisma/client/**"]
    },
    "@calcom/web#build": {
      "dependsOn": ["@calcom/prisma#generate-schemas"],
      "outputs": [".next/**"]
    },
    "@calcom/prisma#db-deploy": {
      "cache": false,
      "dependsOn": [],
      "inputs": ["./schema.prisma", "./migrations/**/*.sql"]
    },
    "@calcom/prisma#db-seed": {
      "cache": false,
      "dependsOn": ["@calcom/prisma#db-deploy"]
    },
    "@calcom/prisma#db-migrate": {
      "cache": false,
      "dependsOn": [],
      "inputs": ["./schema.prisma", "./migrations/**/*.sql"]
    },
    "@calcom/prisma#db-rollback": {
      "cache": false,
      "dependsOn": [],
      "inputs": ["./schema.prisma", "./migrations/**/*.sql"]
    },
    "@calcom/web#start": {
      "cache": false,
      "dependsOn": []
    },
    "type-check": {
      "cache": false
    },
    "type-check:ci": {
      "cache": false
    },
    "@calcom/prisma#post-install": {
      "cache": false,
      "outputs": ["dist/**", "build/**"]
    },
    "post-install": {
      "dependsOn": [],
      "outputs": ["../../node_modules/@prisma/client/**", "../../node_modules/@prisma/admin-client/**"],
      "inputs": ["./schema.prisma", "./prisma/schema.prisma"],
      "env": ["PRISMA_GENERATE_DATAPROXY"]
    },

    "lint": {
      "cache": false
    },
    "lint:fix": {
      "cache": false
    },
    "lint:report": {
      "cache": false,
      "outputs": ["lint-results"]
    },
    "@calcom/platform-types#build": {
      "cache": false,
      "outputs": ["dist/**"]
    },
    "deploy": {
      "cache": false,
      "dependsOn": ["@calcom/prisma#db-deploy"]
    }
  },
