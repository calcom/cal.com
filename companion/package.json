    "expo": "~54.0.0",
    "expo-auth-session": "^7.0.9",
    "expo-constants": "~18.0.10",
    "expo-crypto": "^15.0.7",
    "expo-device": "^8.0.9",
    "expo-glass-effect": "~0.1.6",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.14",
